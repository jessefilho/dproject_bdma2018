docker run -it -d  --name dw_postgres -p 1433:5432 -v /var/lib/docker/volumes -e POSTGRES_USER=musicbrainz -e POSTGRES_PASSWORD=musicbrainz arey/musicbrainz-database

docker run -it --link dw_postgres:postgresql -e POSTGRES_USER=musicbrainz -e POSTGRES_PASSWORD=musicbrainz --rm arey/musicbrainz-database /create-database.sh

